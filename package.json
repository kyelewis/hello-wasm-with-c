{
  "name": "@kyedoesdev/hello-wasm-with-c",
  "version": "0.1.0",
  "description": "Hello, World for WASM with C",
  "main": "src/web/index.ts",
  "scripts": {
    "start": "build && cd src/web && vite",
    "build": "clang --target=wasm32 --no-standard-libraries -Wl,--export-all, -Wl,--no-entry -o out/hello.wasm src/c/hello.c"
  },
  "author": "Kye Lewis <kye@kyedoesdev.com>",
  "license": "MIT",
  "devDependencies": {
    "vite": "3.0.0"
  }
}
