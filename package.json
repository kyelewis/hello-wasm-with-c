{
  "name": "@kyedoesdev/hello-wasm-with-c",
  "version": "0.1.0",
  "description": "Hello, World for WASM with C",
  "main": "src/web/index.ts",
  "scripts": {
    "start": "npm run build && vite src/js",
    "build": "clang-11 --target=wasm32 -ffreestanding -fno-builtin -O3 --no-standard-libraries -Wl,--export-all -Wl,--no-entry -Wl,--allow-undefined  -o out/hello.wasm src/c/hello.c"
  },
  "author": "Kye Lewis <kye@kyedoesdev.com>",
  "license": "MIT",
  "devDependencies": {
    "vite": "3.0.0"
  }
}
